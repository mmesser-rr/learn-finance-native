      "dependsOn": [
        {
          "category": "api",
          "resourceName": "theplayerscompany",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        },
        {
          "category": "function",
          "resourceName": "PlayerVerifyService",
          "attributes": [
            "Name"
